<!DOCTYPE html>
<html>

  <head>             
    <meta name="layout" content="main"> 
    <script> 
            $(document).ready(function() {  
  $('form').submit(function(){
      $('.btn-primary').attr('disabled', 'disabled');
  });
  });
    function imgDel() { 
                $('#img').attr("src","data:image/png;base64,");
                $('#img').css("opacity","0.4");
                $('#iDlt').val("Y");
            }
            $(window).load(function() {                                                       
                $('#pswd').prop('required', false);
                $('#usrName').prop('readOnly', true);
                $('#usrName').css("cursor","not-allowed");  

            });  
            
          function Validate() {           
              if (document.getElementById("pswd").value != document.getElementById("cpswd").value) {
                  document.getElementById("pswdvalidation").innerHTML="Do not match!!";
                 return false;
              }
              document.getElementById("pswdvalidation").innerHTML="";
             return true;
            }
          
        
        
    </script>


  </head>   
  <body>
    <div class="box box-primary">
      <div class="box-header with-border">
        <legend align="center">Employee</legend>
      </div>
      <!-- /.box-header -->
      <!-- form start -->
      <g:form url="[resource:hrEmpMstrInstance, action:'update']" method="POST" enctype="multipart/form-data">
        <ul class="nav nav-tabs">
          <li class="active"><a data-toggle="tab" href="#Profile">Profile</a></li>                  
          <li><a data-toggle="tab" href="#lform">Login Credentials</a></li>                  
        </ul>
        <div class="tab-content">
          <div id="Profile" class="tab-pane fade in active">
            <legend align="center">Profile</legend>
            <g:render template="form"/>
          </div>
          <div id="lform" class="tab-pane fade">
            <legend align="center">Login Credentials</legend>
            <g:render template="login"/>
          </div>                     
        </div>
        <!---->   
        <!--<div id="lform" style="display: none;"></div>-->
        <input style="display: none" id="iDlt" name="iDlt" value="null"></input>
        <!-- /.box-body -->              
        <div class="box-footer">                               
          <g:actionSubmit class="btn btn-primary" action="update" onclick="return Validate()" value="Update" />
          <button class="btn btn-danger" onclick="return confirm('Are you sure to Delete!!?')"  name="del" value="1" >  Delete  </button>
          <g:link class="btn btn-info" action="index">Back</g:link>

        </div>   


      </g:form>

    </div>        
  </body>



</html>
